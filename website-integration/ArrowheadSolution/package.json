{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/sync-admin-config.mjs && NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "prebuild": "node scripts/sync-cloudflare-config.mjs && node scripts/sync-admin-config.mjs && node scripts/generate-seo.mjs",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "postbuild": "node scripts/check-build-admin.mjs",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate": "drizzle-kit generate --config=drizzle.config.cjs",
    "db:push": "drizzle-kit push --config=drizzle.config.cjs",
    "db:seed:blog": "npx tsx scripts/seed-blog.mts",
    "db:seed:audit": "node scripts/audit-blog-seed.mjs",
    "db:rls:apply": "node scripts/apply-rls.mjs",
    "db:rls:verify": "node scripts/verify-rls.mjs",
    "db:rls:verify:rest": "node scripts/verify-rls-rest.mjs",
    "admin:create": "tsx server/scripts/create-admin.ts",
    "test:install": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed --project=chromium",
    "test:e2e:ci": "playwright test --project=chromium",
    "test:unit": "vitest run --passWithNoTests",
    "test:unit:ci": "vitest run --coverage",
    "test:unit:watch": "vitest",
    "test:prod:e2e:access": "PLAYWRIGHT_NO_WEBSERVER=1 playwright test --project=prod-chromium -g \"PROD Access\"",
    "test:prod:e2e:smoke": "PLAYWRIGHT_NO_WEBSERVER=1 playwright test --project=prod-chromium --grep 'Blog|robots|rss|sitemap'",
    "test:prod:e2e:lead": "PLAYWRIGHT_NO_WEBSERVER=1 playwright test --project=prod-chromium -g \"PROD Lead Magnet API\"",
    "test:prod:e2e:lead:headonly": "PLAYWRIGHT_NO_WEBSERVER=1 E2E_SKIP_LEAD_POSTS=1 playwright test --project=prod-chromium -g \"PROD Lead Magnet API\"",
    "cf:deploys": "node scripts/cf-pages-deploys.mjs",
    "cf:purge": "node scripts/cf-purge-zone.mjs",
    "cf:fetch": "node scripts/cf-access-fetch.mjs",
    "cf:access:list-apps": "node scripts/cf-access-list-apps.mjs",
    "cf:access:list-tokens": "node scripts/cf-access-list-service-tokens.mjs",
    "cf:access:upsert-policy": "node scripts/cf-access-upsert-policy.mjs",
    "validate:blog": "node scripts/validate-blog.mjs",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "eslint . --ext .ts,.tsx --max-warnings=0",
    "db:up": "docker compose -f docker-compose.dev.yml up -d",
    "db:down": "docker compose -f docker-compose.dev.yml down -v",
    "dev:local": "DB_SSL_DISABLE=1 DATABASE_URL=postgresql://postgres:postgres@localhost:54322/arrowhead_dev npm run dev"
  },
