# Frontend (Vite)
VITE_TURNSTILE_SITE_KEY=
VITE_ADMIN_API_BASE=/api

# Supabase (Frontend Auth)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Production E2E tests
PLAYWRIGHT_PROD_BASE_URL=https://project-arrowhead.pages.dev
# Skip production POST tests when Turnstile is enforced
E2E_SKIP_LEAD_POSTS=1

# Optional Cloudflare Access for protected routes
CF_ACCESS_CLIENT_ID=
CF_ACCESS_CLIENT_SECRET=

# Stripe Configuration
# See: docs/STRIPE_SETUP_GUIDE.md for setup instructions
# Use TEST keys (sk_test_, pk_test_) for local development
# Use LIVE keys (sk_live_, pk_live_) for production
STRIPE_SECRET_KEY=sk_test_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
# Team Edition Price ID ($49/month recurring)
STRIPE_PRICE_ID_TEAM_PLAN=price_...
# Base URL for redirect after checkout
PUBLIC_SITE_URL=http://localhost:5000

# Logging & Error Tracking
# Sentry DSN for error monitoring (optional, disabled in development by default)
SENTRY_DSN=
# Force enable Sentry in development (optional)
SENTRY_FORCE_ENABLE=0
# Release version for Sentry tracking (auto-detected from package.json if not set)
SENTRY_RELEASE=

# Cloudflare Pages Functions (set in Cloudflare environment, not locally)
# SUPABASE_URL=
# SUPABASE_SERVICE_ROLE=
# PUBLIC_SITE_URL=https://project-arrowhead.pages.dev
# ALLOWED_ORIGINS=
# SUPABASE_LEADS_TABLE=leads
# TURNSTILE_SECRET_KEY=
# TURNSTILE_REQUIRED=true
